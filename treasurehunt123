import hmac
import hashlib
import sys
from hashlib import sha256

#all credit to @JohnCantrell97's breakdown available here (ruby): https://gist.github.com/johncantrell97/bbab69bbde03d22eb8323fd94cd46db0
#you need to first download a word list, the one I found online had 497k words and it helped with the first two passwords but the thirdpassword was not present. This resource has every possible word you can ask for 
#http://ftp.icm.edu.pl/packages/wordlists/ 


def main():
    #enter here your keys
    K1 = ""
    K2 = ""
    K3 = ""

    #prepare the keys
    K1_encryptedHTML = K1[64:]
    K2_encryptedHTML = K2[64:]
    K3_encryptedHTML = K3[64:]
    K1_encryptedHMAC = K1[:64]
    K2_encryptedHMAC = K2[:64]
    K3_encryptedHMAC = K3[:64]
    
        
    f = open('words.txt', 'rU')
    
        
    for i in f:
        i = i.strip()
        sha256passphrase = hashlib.sha256(i.lower()).hexdigest()
        K1_decryptedHMAC = hmac.new(sha256passphrase, K1_encryptedHTML, sha256).hexdigest()
        K2_decryptedHMAC = hmac.new(sha256passphrase, K2_encryptedHTML, sha256).hexdigest()
        K3_decryptedHMAC = hmac.new(sha256passphrase, K3_encryptedHTML, sha256).hexdigest()
        
        if K1_decryptedHMAC == K1_encryptedHMAC:
            print "The password 1 is:", i
            
        if K2_decryptedHMAC == K2_encryptedHMAC:
            print "The password 2 is:", i
            
        if K3_decryptedHMAC == K3_encryptedHMAC:
            print "The password 3 is:", i        
            
        

    f.close()
if __name__ == '__main__':
    main()
